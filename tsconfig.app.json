{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@app/*": [ "app/*" ],
      "@pages/*": [ "app/pages/*" ],
      "@shared/*": [ "app/shared/*" ],
      "@assets/*": [ "assets/*" ],
      "@env/*": [ "environments/*" ]
    },
    "outDir": "./out-tsc/app",
    "types": []
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
